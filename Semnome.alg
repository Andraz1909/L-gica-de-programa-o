algoritmo "consumo de combustivel"

var
     distancia, combustivel, consumo: real
     
inicio

escreva ("distancia percorrida em Km:")
leia (distancia)

escreva ("combustivel gasto:")
leia (combustivel)

consumo <- distancia / combustivel
escreval (" O consumo Medio de gasolina é " , consumo: 5:3)
fimalgoritmo
