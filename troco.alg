algoritmo "TROCO"

var  PRECO, DinheiroRecebido, troco : real

     quantidade: inteiro
inicio
escreva ("valor de quantidade:")
leia (quantidade)

escreva ("valor de preco:")
leia (preco)

escreva ("valor de dinheiroRecebido:")
leia (dinheiroRecebido)

Troco <-  dinheiroRecebido - preco * quantidade
Escreval ("Troco = ", troco: 4:2)

fimalgoritmo
