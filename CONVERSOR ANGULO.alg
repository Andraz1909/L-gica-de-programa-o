algoritmo "conversor ANGULO"
// Função :
// Autor :
// Data : 27/01/2025
// Seção de Declarações 
var       angulo, S: Real

inicio
                     Escreva("informe um angulo:")
                     Leia(angulo)
                     S <- Sen (GraupRad (angulo))
                     Escreva ("O seno de ", angulo, " e igual a ", S)
                     
fimalgoritmo
